{
  "name": "e-commerce",
  "description": "repositorio multipaquete backend-frontend para final de react-native",
  "private": true,
  "scripts": {
    "start": "concurrently \"npm run start:frontend\" \"npm run start:backend\"",
    "start:frontend": "cd frontend && npm start",
    "start:backend": "cd backend && npm start",
     "install-dependencies": "concurrently \"npm install --prefix frontend\" \"npm install --prefix backend\""
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}

